{
  "name": "gaman",
  "version": "0.0.40",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs_ts": "tsc --project tsconfig.cjs.json && tsc-alias -p tsconfig.cjs.json",
    "build:esm_ts": "tsc --project tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "build:cjs": "node build-cjs.js",
    "build:esm": "node build-esm.js",
    "build": "npm run clean  && npm run build:cjs && npm run build:esm && tsc",
    "build:bun": "npm run clean && bun ./build/build.ts",
    "test": "npm run build:bun && bun --watch test/src/main",
    "release": "npm run build:bun && npm publish",
    "lint:src": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix"
  },
  "author": "angga7togk",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/7TogkID/gaman"
  },
  "bugs": {
    "url": "https://github.com/7TogkID/gaman/issues"
  },
  "dependencies": {
    "@gaman/cli": "0.0.12",
    "cookie": "^1.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/bun": "^1.2.18",
    "@types/busboy": "^1.5.4",
    "@types/ejs": "^3.1.5",
    "@types/ws": "^8.18.1",
    "arg": "^5.0.2",
    "busboy": "^1.6.0",
    "dotenv": "^17.2.0",
    "ejs": "^3.1.10",
    "eslint": "^9.31.0",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.16",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "ws": "^8.18.2"
  },
  "private": true
}
